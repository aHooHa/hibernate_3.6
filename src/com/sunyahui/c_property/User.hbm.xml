<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">



<hibernate-mapping package="com.sunyahui.c_property">

	<class name="User" table="tt_user1">
		<id name="id" type="int" column="id">
			<generator class="native">
				<param name="sequence">tt_user_t1</param>
			</generator>
		</id>
		<property name="name" type="string" column="name" length="20" />
		<property name="age" type="int" column="age"></property>
		<property name="time" type="date" column="time1"></property>
		<property name="desci" ></property>
		<!--<property name="photo" ></property> -->
		
	</class>
</hibernate-mapping>
